.site-footer{background-color:#fff;border-top:1px solid #000;padding:20px 0}.site-footer .container{display:flex;justify-content:center;align-items:center;flex-direction:column}.footer-menu{list-style:none;margin:0;padding:0;display:flex;gap:20px;justify-content:center;margin-bottom:10px}.footer-menu li{display:inline-block}.footer-menu li a{text-decoration:none;color:#000;font-size:16px;font-weight:700}.footer-menu li a:hover{color:#e71d36}.site-footer p{margin:0;font-size:14px;font-weight:700;text-align:center;width:100%}#menu-item-50{padding-left:130px;padding-right:130px}.non-clickable a{pointer-events:none;color:inherit;text-decoration:none}.non-clickable a:hover{color:inherit;text-decoration:none}